color xyline 			     "204 102 119" 

// axis and grad
axisstyle                    horizontal_withgrid
axisstyle horizontal_default horizontal_withgrid
axisstyle vertical_default   vertical_withgrid

// tick
anglestyle vertical_tick     horizontal
gsize label                  medium
gsize tick_label             medium

// legend
gsize legend_key_ysize      6
gsize legend_key_xsize      6
